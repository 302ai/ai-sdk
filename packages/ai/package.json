{
  "name": "@302ai/ai-sdk",
  "version": "0.2.13",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"./src/**/*.ts*\" \"./tsup.config.ts\"",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "test": "pnpm test:node && pnpm test:edge",
    "test:edge": "vitest --config vitest.edge.config.js --run",
    "test:node": "vitest --config vitest.node.config.js --run"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^2.0.0-beta.47",
    "@ai-sdk/provider": "3.0.0-beta.26",
    "@ai-sdk/provider-utils": "4.0.0-beta.46",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@302ai/ai-tsconfig": "workspace:*",
    "@types/node": "^18",
    "tsup": "^8.5.1",
    "typescript": "5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14",
    "zod": "^4.1.13"
  },
  "peerDependencies": {
    "zod": "^4.1.13"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/302ai/ai-sdk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/302ai/ai-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/302ai/ai-sdk/issues"
  },
  "keywords": [
    "ai",
    "302ai"
  ],
  "packageManager": "pnpm@10.23.0"
}
